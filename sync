#!/bin/bash
cd ~
sudo apt install git aria2 -y
git clone https://gitlab.com/OrangeFox/misc/scripts
cd scripts
sudo bash setup/android_build_env.sh
sudo bash setup/install_android_sdk.sh
mkdir ~/OrangeFox
cd ~/OrangeFox
repo init -u https://gitlab.com/OrangeFox/Manifest.git -b fox_9.0
repo sync -j8 --force-sync
pwd
ls
cd ~/OrangeFox
##delete these lines, and replace with your own lines of device sauce
git clone https://github.com/TeamWin/android_device_asus_X00P device/asus/X00P

#git clone https://github.com/TwrpBuilder/android_vendor_qcom_opensource_commonsys vendor/qcom/opensource/commonsys
