#!/bin/bash
cd ~/OrangeFox
  source build/envsetup.sh
export ALLOW_MISSING_DEPENDENCIES=true
export FOX_USE_TWRP_RECOVERY_IMAGE_BUILDER=1
export LC_ALL="C"
lunch omni_X00P-eng
mka recoveryimage
sudo su
curl --upload-file /home/ci/OrangeFox/out/target/product/X00P/OrangeFox-Unofficial-TBX304.zip https://transfer.sh
cd  /home/ci/OrangeFox
curl --upload-file /out/target/product/X00P/OrangeFox-Unofficial-TBX304.zip https://transfer.sh
curl --upload-file out/target/product/X00P/OrangeFox-Unofficial-TBX304.zip https://transfer.sh
curl --upload-file ./out/target/product/X00P/OrangeFox-Unofficial-TBX304.zip https://transfer.sh
echo "/nUploaded All files successfully!"
