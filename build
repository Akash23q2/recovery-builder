#!/bin/bash
cd ~/OrangeFox
  source build/envsetup.sh
export ALLOW_MISSING_DEPENDENCIES=true
export FOX_USE_TWRP_RECOVERY_IMAGE_BUILDER=1
export LC_ALL="C"
lunch omni_TBX304-eng
mka recoveryimage
sudo su
curl --upload-file /home/ci/OrangeFox/out/target/product/TBX304/OrangeFox-Unofficial-TBX304.zip
cd  /home/ci/OrangeFox
curl --upload-file /out/target/product/TBX304/OrangeFox-Unofficial-TBX304.zip
curl --upload-file out/target/product/TBX304/OrangeFox-Unofficial-TBX304.zip
curl --upload-file ./out/target/product/TBX304/OrangeFox-Unofficial-TBX304.zip
echo "/nUploaded All files successfully!"
