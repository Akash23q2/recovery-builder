#!/bin/bash
cd ~/OrangeFox_10/fox_10.0
  source build/envsetup.sh
export ALLOW_MISSING_DEPENDENCIES=true
export OF_PATCH_AVB20=1
export FOX_USE_TWRP_RECOVERY_IMAGE_BUILDER=1
export LC_ALL="C"
lunch omni_merlin-eng
mka recoveryimage
sudo su
curl --upload-file /home/ci/OrangeFox_10/fox_10.0/out/target/product/merlin/OrangeFox-Unofficial-merlin.zip https://transfer.sh
curl --upload-file home/ci/out/target/product/merlin/OrangeFox-Unofficial-merlin.zip https://transfer.sh
cd ~/home/ci/OrangeFox_10/fox_10.0
curl --upload-file /out/target/product/merlin/OrangeFox-Unofficial-merlin.zip https://transfer.sh
curl --upload-file ./out/target/product/merlin/OrangeFox-Unofficial-merlin.zip https://transfer.sh
curl --upload-file out/target/product/merlin/OrangeFox-Unofficial-merlin.zip https://transfer.sh

echo "/nUploaded All files successfully!"
